set fenc=utf-8
set nobackup
set noswapfile
set autoread
set hidden
set showcmd

set number
set cursorline
set cursorcolumn
set virtualedit=onemore
set smartindent
set visualbell
set showmatch

set tabstop=2
set shiftwidth=2

set clipboard+=unnamed
set backspace=indent,eol,start

syntax enable
set re=0
